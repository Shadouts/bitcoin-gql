{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "./src",
    "module": "es6",
    "noImplicitAny": true,
    "outDir": "./dist/",
    "paths": {
      "Classes/*": ["classes/*"],
      "Models/*": ["models/*"],
      "Types/*": ["types/*"],
